@media (min-width: 992px) {
  #formats .container div > div {
    float: left;
    width: 33.33333333333333%
  }
}

#formats .container div > div h3 {
  text-align: center;
  font-weight: 200
}

#formats .container div > div p {
  color: #999;
  padding: 20px
}

#formats .container div > div hr {
  margin: 0 20px
}

#formats .container div > div:nth-child(1) h3 {
  color: #2dffb0
}

