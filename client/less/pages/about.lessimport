  top: 45px;
  left: 6px
}

#about .container > div > div:nth-child(2) > div i:last-of-type {
  bottom: 45px;
  right: 6px
}

#formats {
  position: relative;
  min-height: 400px
}

#formats .rotated {
  z-index: 1;
  position: absolute;
  width: 110%;
  height: 40px;
  left: -5%;
  top: -29px;
  background: #fff;
  -webkit-transform: rotate(1.1deg);
  -ms-transform: rotate(1.1deg);
  transform: rotate(1.1deg)
}

@media (max-width: 768px) {
  #formats .rotated {
    top: 15px
  }
}

#formats p {
  font-weight: 200
}

#formats .container {
  z-index: 2;
  position: relative
}

#formats .container div {
  margin-left: -15px;
  margin-right: -15px
}

#formats .container div:before, #formats .container div:after {
  content: " ";
  display: table
}

#formats .container div:after {
  clear: both
}

